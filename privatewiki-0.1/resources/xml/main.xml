<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- generated by ThinG, the Thinlet GUI editor -->
<panel background="#ffffff" columns="1" weighty="1">
    <menubar weightx="1">
        <menu name="menu_file" text="File">
            <menuitem accelerator="ctrl N" action="onNewDocument()" icon="/icons/new.png" name="menu_file_new" text="New Document"/>
            <menuitem accelerator="ctrl O" action="notImplemented()" icon="/icons/import.png" name="menu_file_import_file" text="Import file(s)..."/>
            <menuitem action="notImplemented()" icon="/icons/import.png" name="menu_file_import_folder" text="Import folder(s)..."/>
            <menuitem accelerator="ctrl W" action="onExit()" icon="/icons/quit.png" name="menu_file_exit" text="Exit"/>
        </menu>
        <menu name="menu_tools" text="Tools">
            <menuitem action="onSetChangePassword()" icon="/icons/private.png" text="Set/Change password"/>
        </menu>
        <menu name="menu_help" text="Help">
            <menuitem accelerator="F1" action="onAbout()" icon="/icons/info.png" name="menu_help_about" text="About..."/>
        </menu>
    </menubar>
    <panel background="#cccccc" name="toolbar" weightx="1">
        <button action="onNewDocument()" icon="/icons/new.png" name="button_new_document" tooltip="New Document"/>
        <button action="onBack()" icon="/icons/back.gif" name="button_back" tooltip="Back"/>
        <button action="onForward()" icon="/icons/forward.gif" name="button_forward" tooltip="Forward"/>
        <button action="onGoHome()" icon="/icons/home.png" name="button_home" tooltip="Go to home document"/>
        <combobox name="combo_location" perform="onChangeLocation(this)" selected="0" weightx="1" width="400"/>
        <button action="onSearch()" icon="/icons/find.png" name="button_search" tooltip="Search"/>
    </panel>
    <panel gap="4" init="initContentPanel(this)" name="contentPanel" weightx="1" weighty="1"/>
    <panel background="#c4c4c4" border="true" gap="2" name="statusbar" weightx="1">
        <label name="statusbar_status" text=" " weightx="1"/>
    </panel>
</panel>
